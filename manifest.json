{
  "manifest_version": 3,
  "name": "ChromeBee",
  "version": "1.0",
  "description": "Hover on links to shake pollen off the flower",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./lib/p5.js", "./main.js", "./sketch.js"],
      "css": ["./style.css"]
    }
  ],
  "action": {
    "default_title": "Title",
    "default_icon": {
      "16": "assets/bee.png",
      "48": "assets/bee.png",
      "128": "assets/bee.png"
    }
  }
}
